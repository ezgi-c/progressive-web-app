{
    "short_name": "PWA Camera",
    "name": "PWA Camera",
    "icons": [
      {
        "src": "camera-icon(s).png",
        "type": "image/png",
        "sizes": "128x128"
      },
      {
        "src": "camera-icon(l).png",
        "type": "image/png",
        "sizes": "512x512"
      }
    ],
    "start_url": "/index.html",
    "display": "standalone",
    "theme_color": "#008080",
    "background_color": "#000000",
    "scope": "/index.html",
    "serviceworker": {
      "src": "/service-worker.js",
      "scope": "https://pwa-takeapicture.netlify.app/",
      "type": "module"
    }
  }
  